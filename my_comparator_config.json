{
  "// Configuration Examples": "See below for different modes",

  "// ========================================": "",
  "// CASE 1: Two-File Mode": "",
  "// Use this when you have separate CSV files for yes and no cases": "",
  "// ========================================": "",
  "// Uncomment and modify the following fields for two-file mode": "",

  "// input_csv_path_for_yes_case": "./input/yes_sketches.csv",
  "// input_csv_path_for_no_case": "./input/no_sketches.csv",
  "// min_support_for_yes_case": 0.05,
  "// min_support_for_no_case": 0.05,
  "// max_levels": 5,
  "// include_all_level1": true,
  "// output_itemsets_path_yes_case": "./output/comparator_yes_itemsets.csv",
  "// output_itemsets_path_no_case": "./output/comparator_no_itemsets.csv",
  "// output_itemsets_joined": "./output/comparator_joined.csv",
  "// item_separator": " && ",
  "// sketch_lg_k_yes_case": 12,
  "// sketch_lg_k_no_case": 12,
  "// excluded_items": ["item1", "item2"],
  "// use_equi_join": false,
  "// filter_item": "region=North",

  "// ========================================": "",
  "// CASE 2a: Single-File Mode with One Target": "",
  "// Use this when you want to split one CSV using a target item": "",
  "// YES = intersection with target, NO = A-not-B with target": "",
  "// ========================================": "",
  "// To use this mode, comment out the two-file fields above and uncomment below": "",

  "input_csv_path": "./input/DU_sketch_14NominalVal.csv",
  "min_support_for_yes_case": 0.001,
  "min_support_for_no_case": 0.001,
  "max_levels": 5,
  "include_all_level1": true,
  "output_itemsets_path": "./output/DU14_comparator_joined.csv",
  "item_separator": " && ",
  "sketch_lg_k": 14,
  "target_item_1": "tripOutcome=good",
  "excluded_items": null,
  "use_equi_join": true,
  "filter_item": null,

  "// ========================================": "",
  "// CASE 2b: Single-File Mode with Two Targets": "",
  "// Use this when you have mutually exclusive target items": "",
  "// YES = intersection with target_item_1, NO = intersection with target_item_0": "",
  "// ========================================": "",
  "// To use this mode, comment out the two-file fields above and uncomment below": "",

  "// input_csv_path": "./input/sketches.csv",
  "// min_support_for_yes_case": 0.03,
  "// min_support_for_no_case": 0.02,
  "// max_levels": 5,
  "// include_all_level1": true,
  "// output_itemsets_path": "./output/comparator_joined.csv",
  "// item_separator": " && ",
  "// sketch_lg_k": 12,
  "// target_item_1": "outcome=positive",
  "// target_item_0": "outcome=negative",
  "// excluded_items": ["item1", "item2"],
  "// use_equi_join": false,
  "// filter_item": "region=North",

  "// ========================================": "",
  "// Field Descriptions": "",
  "// ========================================": "",

  "// Two-File Mode Fields:": "",
  "//   input_csv_path_for_yes_case: Path to yes case sketches CSV": "",
  "//   input_csv_path_for_no_case: Path to no case sketches CSV": "",
  "//   output_itemsets_path_yes_case: Output path for yes itemsets": "",
  "//   output_itemsets_path_no_case: Output path for no itemsets": "",
  "//   sketch_lg_k_yes_case: Sketch size parameter for yes case (4-26)": "",
  "//   sketch_lg_k_no_case: Sketch size parameter for no case (4-26)": "",

  "// Single-File Mode Fields:": "",
  "//   input_csv_path: Path to single sketches CSV": "",
  "//   output_itemsets_path: Output path for joined itemsets": "",
  "//   sketch_lg_k: Sketch size parameter (4-26)": "",
  "//   target_item_1: Target item for yes case (mandatory)": "",
  "//   target_item_0: Target item for no case (optional)": "",

  "// Common Fields:": "",
  "//   min_support_for_yes_case: Min support for yes (0.0-1.0)": "",
  "//   min_support_for_no_case: Min support for no (0.0-1.0)": "",
  "//   max_levels: Maximum itemset depth": "",
  "//   include_all_level1: Include all level 1 items (true/false)": "",
  "//   output_itemsets_joined: Path to joined output CSV": "",
  "//   item_separator: Separator for items in itemsets (default ' && ')": "",
  "//   excluded_items: List of items to exclude from analysis (optional, e.g., ['item1', 'item2'])": "",
  "//   use_equi_join: If true, only output itemsets in both yes and no cases (default: false)": "",
  "//   filter_item: Pre-filter item to intersect all sketches before splitting (optional, e.g., 'region=North')": ""
}
